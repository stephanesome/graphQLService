extend type Query {
    authors(bookNumber: Int!): [Author]
}

type Author { bookNumber: Int!
    firstName: String!
    lastName: String!
}

input CreateAuthorInput {
    bookNumber: Int!
    firstName: String!,
    lastName:String!
}

extend type Mutation {
    newAuthor(createAuthorInput: CreateAuthorInput!) : Author!
}
